{
  "name": "apollo-bridge-link-example",
  "version": "1.0.0",
  "description": "demo of client side graphql bridge to rest api",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --watch --port 3003 --config webpack/webpack.config.js",
    "server": "json-server --watch db.json",
    "lint": "eslint ./src",
    "test": "cross-env NODE_ENV=test ava",
    "test:watch": "npm test -- --watch -v"
  },
  "author": "dacz",
  "license": "MIT",
  "repository": "https://github.com/dacz/apollo-bridge-link-example",
  "browserslist": [
    "> 3%",
    "Safari >= 8",
    "IE 11"
  ],
  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-ramda": "^1.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.0",
    "eslint-config-dacz": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "apollo-bridge-link": "^1.0.2",
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "dataloader": "^1.3.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.9.0",
    "isomorphic-fetch": "^2.2.1",
    "json-server": "^0.12.1",
    "query-string": "^5.0.1",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0"
  }
}
